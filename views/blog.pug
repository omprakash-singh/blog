doctype html
html(lang="en")
     head
          meta(charset="UTF-8")
          meta(http-equiv="X-UA-Compatible", content="IE=edge")
          meta(name="viewport", content="width=device-width, initial-scale=1.0")
          title || college blog ||
          link(rel="stylesheet", href="bloguser.css")
     body 
     .navbar 
         h1 college 
            span blog 
         ul 
          li 
           a(href="/blogpost") post 
          li 
            a(href="/user") user 
     
     .container 
         each d in data
             .contain 
                  h1=d.name
                  p=d.post
                  h3 Date: 
                      span= d.date